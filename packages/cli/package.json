{
  "name": "css-panda",
  "version": "0.0.1",
  "description": "The types for css panda",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "Segun Adebayo <joseshegs@gmail.com>",
  "bin": {
    "panda": "dist/cli-default.js"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/cli-*.ts src/index.ts --format=esm,cjs --dts",
    "build-fast": "tsup src/cli-*.ts src/index.ts --format=esm,cjs --no-dts",
    "dev": "pnpm build-fast -- --watch"
  },
  "files": [
    "dist",
    "bin"
  ],
  "dependencies": {
    "@css-panda/node": "workspace:*",
    "@css-panda/error": "workspace:*",
    "@css-panda/shared": "workspace:*",
    "@css-panda/types": "workspace:*",
    "@css-panda/logger": "workspace:*",
    "cac": "6.7.14"
  },
  "devDependencies": {
    "kleur": "^4.1.5"
  }
}
