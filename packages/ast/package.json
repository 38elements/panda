{
  "name": "@css-panda/ast",
  "version": "0.0.1",
  "description": "The static parser for css panda",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "Segun Adebayo <joseshegs@gmail.com>",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format=esm,cjs --dts",
    "build-fast": "tsup src/index.ts --format=esm,cjs --no-dts",
    "dev": "pnpm build-fast -- --watch",
    "minify": "tsx watch ./__sandbox__/minify.ts",
    "patterns": "tsx watch ./__sandbox__/extract-pattern.ts"
  },
  "dependencies": {
    "@babel/parser": "7.19.1",
    "@babel/generator": "7.19.0",
    "@babel/traverse": "7.19.1",
    "@babel/types": "7.19.1",
    "@swc/core": "1.2.248",
    "@css-panda/logger": "workspace:*",
    "@css-panda/is-valid-prop": "workspace:*",
    "@css-panda/types": "workspace:*",
    "ts-pattern": "4.0.5",
    "lodash.merge": "4.6.2"
  },
  "devDependencies": {
    "@css-panda/fixture": "workspace:*",
    "@types/lodash.merge": "4.6.7",
    "@types/babel__traverse": "7.18.1"
  },
  "files": [
    "dist"
  ]
}
