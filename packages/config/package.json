{
  "name": "@css-panda/config",
  "version": "0.0.1",
  "description": "Find and load panda config",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "Segun Adebayo <joseshegs@gmail.com>",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm tsup src/index.ts --format=esm,cjs --shims --dts",
    "build-fast": "pnpm tsup src/index.ts --format=esm,cjs --shims --no-dts",
    "dev": "pnpm build-fast -- --watch"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@css-panda/logger": "workspace:*",
    "@css-panda/shared": "workspace:*",
    "@css-panda/types": "workspace:*",
    "esbuild": "0.15.7",
    "look-it-up": "2.1.0"
  }
}
