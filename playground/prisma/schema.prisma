generator client {
  provider = "prisma-client-js"
}

// Using SQLite for local development
datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
}

model Session {
  id        String   @id
  code      String
  theme     String   @default("")
  view      String   @default("code")
  createdAt DateTime @default(now())
}
